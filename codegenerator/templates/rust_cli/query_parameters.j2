
/// Query parameters
#[derive(Args)]
struct QueryParameters {
{%- for param in type_manager.parameters.values() %}
{%- if param.location == "query" %}
    {{ macros.docstring(param.description, indent=4) }}
    {{ param.clap_macros }}
    {{ param.local_name}}: {{ param.type_hint }},
{%- endif %}
{%- endfor %}
}
